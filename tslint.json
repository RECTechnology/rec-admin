{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      false
    ]
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      false
    ],
    "max-classes-per-file": [
      true,
      6
    ],
    "variable-name": [
      true,
      "allow-snake-case"
    ],
    "interface-name": [
      false
    ],
    "jsdoc-format": [
      false
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ]
  },
  "rulesDirectory": []
}