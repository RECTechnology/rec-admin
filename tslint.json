{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-etc"
  ],
  "jsRules": {
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      false
    ]
  },
  "rules": {
    "deprecation": {
      "severity": "warning"
    },
    "no-unused-declaration": true,
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      false
    ],
    "max-classes-per-file": [
      true,
      6
    ],
    "variable-name": [
      true,
      "allow-snake-case"
    ],
    "interface-name": [
      false
    ],
    "jsdoc-format": [
      false
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ]
  },
  "rulesDirectory": []
}